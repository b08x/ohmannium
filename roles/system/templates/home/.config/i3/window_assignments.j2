#############################################################################
#----------------------------- Workspace Outputs ---------------------------#
#############################################################################

{% if i3.workspaces != 'default' %}
{% for ws in i3.workspaces %}
workspace {{ ws }}
{% endfor %}
{% endif %}

#############################################################################
