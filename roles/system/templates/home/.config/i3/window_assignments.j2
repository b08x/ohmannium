assign [class="^calfjackhost"] $ws1
assign [title="VU\ Meter\ \(Stereo\)"] $ws1
# assign [title="^EBU R128 Meter$"] $ws8
assign [title="^x42-dpl"] $ws1
assign [title="jkmeter"] $ws1
# assign [title="zita-mu1-0.3.3"] $ws1
assign [title="zita-rev1"] $ws1


# assign [class="(?i)pulsar"] $ws5
assign [class="Bipscript\ IDE"] $ws5
assign [class="Sonic\ Pi"] $ws5

assign [class="Guitarix"] $ws5
assign [class="Non-Sequencer"] $w5
assign [class="^FMIT$"] $ws5
assign [class="^Hydrogen"] $ws5
assign [class="^Seq192"] $ws5
assign [class="^Timemachine$"] $ws5

assign [class="Mixxx"] $ws7


assign [class="Jack_mixer"] $ws8
assign [class="Non-Mixer"] $ws8

{% if i3.assignments != 'default' %}
{% for app in i3.assignments %}
assign {{ app }}
{% endfor %}
{% else %}
assign [class="Jalv.gtk" title="Helm"] $ws6
assign [class="qmidiarp"] $ws6
assign [class="REAPER"] $ws9
assign [class="Qsampler"] $ws10
assign [class="^Patchage"] $ws10
assign [class="Jalv.select"] $ws10
assign [class="^RaySession$"] $ws10
{% endif %}
